{
  "name": "questify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn generate:prisma && next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf dist",
    "generate": "yarn pal g && npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json schema/schema",
    "db-dev": "prisma migrate dev"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@apollo/server": "^4.9.5",
    "@paljs/nexus": "^6.0.7",
    "@paljs/types": "^6.0.7",
    "@prisma/client": "^5.6.0",
    "bcryptjs": "^2.4.3",
    "graphql": "^16.8.1",
    "graphql-middleware": "^6.1.35",
    "graphql-shield": "^7.6.5",
    "jsonwebtoken": "^9.0.2",
    "next": "14.0.3",
    "nexus": "^1.3.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^4.1.0",
    "@paljs/cli": "^6.0.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5"
  }
}
